{
    "name": "StarWave",
    "short_name": "StarWave",
    "id": "./",
    "version": "0.0.1",
    "start_url": "./",
    "scope": "./",
    "description": "A media audio recorder",
    "display": "fullscreen",
    "display_override": [
        "fullscreen",
        "standalone",
        "tabbed",
        "window-controls-overlay"
    ],
    "orientation": "portrait-primary",
    "background_color": "#191919",
    "theme_color": "#191919",
    "icons": [{
        "src": "./Screenshot_20241216-125252_2.png",
        "sizes": "544x544",
        "purpose": "any"
    }],
    "headers": {
        "Access-Control-Allow-Origin": "*"
    },
    "offline_access": true,
    "serviceworker": {
        "src": "./worker.js",
        "scope": "./",
        "use_cache": true
    }
}
